{
    "name": "rekalogika/file-bundle",
    "description": "Integrates rekalogika/file library into Symfony framework.",
    "homepage": "https://rekalogika.dev/file-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\File\\Bundle\\": "src/"
        }
    },
    "require": {
        "league/flysystem": "^3.16",
        "league/flysystem-local": "^3.16",
        "psr/container": "^2.0",
        "rekalogika/file": "^0.2",
        "rekalogika/file-contracts": "^0.2",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/http-kernel": "^6.2"
    },
    "require-dev": {
        "symfony/routing": "^6.2"
    },
    "suggest": {
        "rekalogika/file-image": "Allows easy resizing image",
        "rekalogika/file-association": "Attach files to entities",
        "rekalogika/file-server": "Temporary URL generator for FileInterface",
        "rekalogika/file-symfony-bridge": "Adapters for Symfony components"
    }
}
